<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
    
<div id="modal_inactive" class="modal modal_confirm ">
	<h4 class="modal-header">Inactivar Usuario</h4>
	<div class="modal-content">   
		<p>¿Está seguro que desea inactivar este usuario?</p>
	</div>
	<div class="modal-footer">
		<div class="row">
			<div class="col s3"></div>
			<div class="col s3">		
				<a class="modal-close waves-effect waves-light btn-small red">Cerrar<i class="material-icons left">clear</i></a>
			</div>
			<div class="col s3">
				<a class="waves-effect waves-light btn-small blue">Aceptar<i class="material-icons left">check</i></a>
			</div>
			<div class="col s3"></div>
		</div>
	</div>
</div>